{
  "name": "point-of-vue",
  "version": "0.0.4",
  "description": "state devtools for Vue and the Composition API",
  "main": "dist/point-of-vue.cjs.js",
  "repository": "https://github.com/oslabs-beta/point-of-vue",
  "author": "Point of Vue ",
  "contributors": [
    "Shelby Neuman",
    "Tony Lei",
    "Tristen Francis",
    "Will Robson"
  ],
  "license": "MIT",
  "module": "dist/point-of-vue.mjs",
  "unpkg": "dist/point-of-vue.iife.js",
  "jsdelivr": "dist/point-of-vue.iife.js",
  "exports": {
    ".": {
      "browser": "./dist/point-of-vue.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/point-of-vue.prod.cjs",
          "development": "./dist/point-of-vue.mjs",
          "default": "./dist/point-of-vue.mjs"
        },
        "require": {
          "production": "./dist/point-of-vue.prod.cjs",
          "development": "./dist/point-of-vue.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/point-of-vue.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public", 
    "devtsc": "tsc --watch -d",
    "build:esbuild": "rimraf dist && tsc -d --emitDeclarationOnly && node ./esbuild.js",
    "build:rollup": "rimraf dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@vue/devtools-api": "^6.0.0-beta.14",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.4.0",
    "vue-demi": "*"
  },
  "peerDependencies": {
    "typescript": ">=4.4.4",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.19.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "npm-run-all": "^4.1.5",
    "pascalcase": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "serve": "^13.0.2",
    "typescript": "4.7.2",
    "vue": "^3.2.36",
    "@types/node": "17.0.36",
    "@vue/compiler-sfc": "3.2.36",
    "esbuild": "0.14.42",
    "esbuild-plugin-vue": "0.2.4"
  }
}
